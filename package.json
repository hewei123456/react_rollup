    "dev:main": "cross-env file=main rollup -c rollup.dev.config.js -w",
    "build:main": "cross-env file=main rollup -c rollup.prod.config.js",
    "dev:webrtc": "cross-env file=webrtc rollup -c rollup.dev.config.js -w",
    "build:webrtc": "cross-env file=webrtc rollup -c rollup.prod.config.js",
    "electron": "nodemon --exec \"electron .\"",
    "electron:pack": "electron-builder --dir",
    "electron:dist": "electron-builder"
